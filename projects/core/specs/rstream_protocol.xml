<!-- A general real data protocol   -->
<Protocol>

  <Operation Name="data" >
    <!-- Variable size frames with 16 bit real data values.  Format:Qs0.15 -->
    <!-- Because sequences can be zero length, the protocol summary attribute
         ZeroLengthMessages="true" is inferred. -->
      <Argument name="real" type="Short" SequenceLength="4096" />
  </Operation>

</Protocol>

